{
  "name": "cdn_properties",
  "version": "0.0.1",
  "description": "FastEdge CDN application for integration tests - property access control",
  "type": "module",
  "scripts": {
    "build": "npm-run-all -s clean build:all copy:all clean",
    "build:all": "npm-run-all -p build:invalid-method-write build:valid-path-write build:valid-url-write build:valid-host-write build:valid-query-write build:invalid-scheme-write build:invalid-geolocation-write build:valid-response-status-read build:invalid-response-status-write build:valid-nginx-log-write",
    "build:invalid-method-write": "asc assembly/invalid-method-write.ts --target release --outFile build/invalid-method-write.wasm --textFile build/invalid-method-write.wat --sourceMap --optimize",
    "build:valid-path-write": "asc assembly/valid-path-write.ts --target release --outFile build/valid-path-write.wasm --textFile build/valid-path-write.wat --sourceMap --optimize",
    "build:valid-url-write": "asc assembly/valid-url-write.ts --target release --outFile build/valid-url-write.wasm --textFile build/valid-url-write.wat --sourceMap --optimize",
    "build:valid-host-write": "asc assembly/valid-host-write.ts --target release --outFile build/valid-host-write.wasm --textFile build/valid-host-write.wat --sourceMap --optimize",
    "build:valid-query-write": "asc assembly/valid-query-write.ts --target release --outFile build/valid-query-write.wasm --textFile build/valid-query-write.wat --sourceMap --optimize",
    "build:invalid-scheme-write": "asc assembly/invalid-scheme-write.ts --target release --outFile build/invalid-scheme-write.wasm --textFile build/invalid-scheme-write.wat --sourceMap --optimize",
    "build:invalid-geolocation-write": "asc assembly/invalid-geolocation-write.ts --target release --outFile build/invalid-geolocation-write.wasm --textFile build/invalid-geolocation-write.wat --sourceMap --optimize",
    "build:valid-response-status-read": "asc assembly/valid-response-status-read.ts --target release --outFile build/valid-response-status-read.wasm --textFile build/valid-response-status-read.wat --sourceMap --optimize",
    "build:invalid-response-status-write": "asc assembly/invalid-response-status-write.ts --target release --outFile build/invalid-response-status-write.wasm --textFile build/invalid-response-status-write.wat --sourceMap --optimize",
    "build:valid-nginx-log-write": "asc assembly/valid-nginx-log-write.ts --target release --outFile build/valid-nginx-log-write.wasm --textFile build/valid-nginx-log-write.wat --sourceMap --optimize",
    "copy:all": "npm-run-all -p copy:invalid-method-write copy:valid-path-write copy:valid-url-write copy:valid-host-write copy:valid-query-write copy:invalid-scheme-write copy:invalid-geolocation-write copy:valid-response-status-read copy:invalid-response-status-write copy:valid-nginx-log-write",
    "copy:invalid-method-write": "mv build/invalid-method-write.wasm ../../../wasm/cdn-apps/properties/invalid-method-write.wasm",
    "copy:valid-path-write": "mv build/valid-path-write.wasm ../../../wasm/cdn-apps/properties/valid-path-write.wasm",
    "copy:valid-url-write": "mv build/valid-url-write.wasm ../../../wasm/cdn-apps/properties/valid-url-write.wasm",
    "copy:valid-host-write": "mv build/valid-host-write.wasm ../../../wasm/cdn-apps/properties/valid-host-write.wasm",
    "copy:valid-query-write": "mv build/valid-query-write.wasm ../../../wasm/cdn-apps/properties/valid-query-write.wasm",
    "copy:invalid-scheme-write": "mv build/invalid-scheme-write.wasm ../../../wasm/cdn-apps/properties/invalid-scheme-write.wasm",
    "copy:invalid-geolocation-write": "mv build/invalid-geolocation-write.wasm ../../../wasm/cdn-apps/properties/invalid-geolocation-write.wasm",
    "copy:valid-response-status-read": "mv build/valid-response-status-read.wasm ../../../wasm/cdn-apps/properties/valid-response-status-read.wasm",
    "copy:invalid-response-status-write": "mv build/invalid-response-status-write.wasm ../../../wasm/cdn-apps/properties/invalid-response-status-write.wasm",
    "copy:valid-nginx-log-write": "mv build/valid-nginx-log-write.wasm ../../../wasm/cdn-apps/properties/valid-nginx-log-write.wasm",
    "clean": "rm -rf build && mkdir -p build"
  },
  "dependencies": {},
  "devDependencies": {},
  "license": "ISC"
}
